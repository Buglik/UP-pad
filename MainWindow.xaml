<Window x:Class="UP_pad.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP_pad"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <CheckBox Content="Przycisk A" HorizontalAlignment="Left" Margin="678,84,0,0" VerticalAlignment="Top" Name="CheckA" Checked="CheckBox_A"/>
        <CheckBox Content="Przycisk B" HorizontalAlignment="Left" Margin="678,104,0,0" VerticalAlignment="Top"  Name="CheckB" Checked="CheckBox_B"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="678,24,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="106" Name="xAxis"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="678,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="106" x:Name="yAxis"/>
        <InkCanvas HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="678" Name="drawingBoard"/>
        <Button Content="Wyczysc" HorizontalAlignment="Left" Margin="695,230,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>


    </Grid>
</Window>
